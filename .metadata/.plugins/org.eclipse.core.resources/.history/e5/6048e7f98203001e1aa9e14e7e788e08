package Day09;

public class Method03 {

	public static void main(String[] args) {
	
		mul(2);
		
		
	}
	// 정수(2~9까지 중 하나) 해당 단의 구구단 출력 메서드
	// 매개변수 :
	// 리턴타입 :
	// 메서드명 : printMulti
	
	public static void mul(int num) {
		for(int i=1; i<=9; i++) {
			System.out.println(num+"*"+i+"="+(num*i));
		}
		
	}
	

}
